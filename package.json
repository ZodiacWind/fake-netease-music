{
  "name": "fake-netease-music",
  "author": "ccq",
  "version": "1.1.0",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"wait-on tcp:3000 -l && electron main.js\"",
    "build": "npm run build:fe && npm run build:mac && npm run build:win64",
    "build:fe": "vite build",
    "build:mac": "electron-builder --config electron-builder.config.js --config.asar=false",
    "build:win64": "electron-builder --config electron-builder.config.js --config.asar=false --win --x64",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "release:patch": "standard-version -r patch",
    "release:minor": "standard-version -r minor",
    "release:major": "standard-version -r major",
    "postinstall": "husky install"
  },
  "main": "main.js",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.7.1",
    "antd": "^4.18.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "electron-is-dev": "^2.0.0",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@ls-lint/ls-lint": "^1.10.0",
    "@types/lodash": "^4.14.178",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-lazyload": "^3.1.1",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^7.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "electron": "^16.0.6",
    "electron-builder": "^22.14.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.5",
    "postcss": "^8.4.5",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-style-import": "^1.4.1",
    "wait-on": "^6.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "semi": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,html,json,md}": "prettier -w",
    "*.less": "stylelint --fix"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  }
}
